.work
  {% for work in site.data.work-projects %}
  .work-thumb
    %a{ href: "/work/{{ work.url }}" }
      %img{ src: "/images/work/thumbs/{{ work.thumb }}", alt: "{{ work.title }}" }
      {{ work.title }}
  {% endfor %}

